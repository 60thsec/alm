{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "isolatedModules": false,
        "jsx": "react",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "filesGlob": [
        "./**/*.ts",
        "./**/*.tsx",
        "!./node_modules/**/*"
    ],
    "files": [
        "./app/commands/commands.ts",
        "./app/commands/events.ts",
        "./app/tabs/tab.ts",
        "./common/constants.ts",
        "./common/utils.ts",
        "./server.ts",
        "./server/cache/cache.ts",
        "./server/cache/fileListingWorker.ts",
        "./server/cache/startFileListingWorker.ts",
        "./server/cookies.ts",
        "./server/devtime.ts",
        "./server/errorCodes.ts",
        "./server/utils/fsu.ts",
        "./server/utils/workerLib.ts",
        "./typings/chokidar/chokidar.d.ts",
        "./typings/colors/colors.d.ts",
        "./typings/cookie-parser/cookie-parser.d.ts",
        "./typings/express/express.d.ts",
        "./typings/function.d.ts",
        "./typings/glob/glob.d.ts",
        "./typings/globals.d.ts",
        "./typings/mime/mime.d.ts",
        "./typings/minimatch/minimatch.d.ts",
        "./typings/mkdirp/mkdirp.d.ts",
        "./typings/mousetrap/mousetrap.d.ts",
        "./typings/node/node.d.ts",
        "./typings/radium/radium.d.ts",
        "./typings/react/react.d.ts",
        "./typings/rimraf/rimraf.d.ts",
        "./typings/serve-static/serve-static.d.ts",
        "./typings/tsd.d.ts",
        "./typings/v8promise.d.ts",
        "./app/main.tsx",
        "./app/material-ui.tsx",
        "./app/root.tsx",
        "./app/styles/styles.tsx",
        "./app/styles/theme.tsx",
        "./app/tabs/codeTab.tsx",
        "./app/tabs/dashboardTab.tsx",
        "./app/tabs/framework/tabs.tsx",
        "./app/tabs/tabsContainer.tsx",
        "./app/ui.tsx"
    ]
}
